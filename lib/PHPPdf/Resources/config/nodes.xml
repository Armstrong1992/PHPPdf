<?xml version="1.0" encoding="UTF-8"?>
<factory>
	<invoke-args>
		<invoke-arg id="list.enumeration_factory" class="PHPPdf\Node\BasicList\EnumerationStrategyFactory" />
	</invoke-args>
	<nodes>
	    <node name="page" class="PHPPdf\Node\Page">
	        <stylesheet>
	            <attribute name="font-type" value="helvetica" />
	            <attribute name="font-size" value="12px" />
	            <attribute name="font-style" value="normal" />
	            <attribute name="margin" value="40px 40px" />
	            <attribute name="color" value="black" />
	        </stylesheet>
	        <formatters>
	            <post class="PHPPdf\Formatter\FloatFormatter" />
	            <post class="PHPPdf\Formatter\ColumnBreakingFormatter" />
	        </formatters>
	    </node>
	    <node name="dynamic-page" class="PHPPdf\Node\DynamicPage">
	        <formatters>
	            <post class="PHPPdf\Formatter\FloatFormatter" />
	            <post class="PHPPdf\Formatter\PageBreakingFormatter" />
	        </formatters>
	    </node>
	    <node name="div" class="PHPPdf\Node\Container">
	    	<alias>pre</alias>
	    	<alias>br</alias>
	    	<alias>nl</alias>
	        <formatters>
	            <pre class="PHPPdf\Formatter\ConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\StandardDimensionFormatter" />
	            <pre class="PHPPdf\Formatter\FirstPointPositionFormatter" />

	            <post class="PHPPdf\Formatter\ContainerDimensionFormatter" />
	            <post class="PHPPdf\Formatter\StandardPositionFormatter" />
	            <post class="PHPPdf\Formatter\FloatFormatter" />
	            <post class="PHPPdf\Formatter\VerticalAlignFormatter" />
	        </formatters>
	    </node>
	    <node name="ul" class="PHPPdf\Node\BasicList">
	    	<invoke method="setEnumerationStrategyFactory" argId="list.enumeration_factory" />
	    	<stylesheet>
	    		<attribute name="margin-left" value="12px" />
	    	</stylesheet>
	        <formatters>
	            <pre class="PHPPdf\Formatter\ConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\StandardDimensionFormatter" />
	            <pre class="PHPPdf\Formatter\FirstPointPositionFormatter" />
	            <pre class="PHPPdf\Formatter\ListFormatter" />

	            <post class="PHPPdf\Formatter\ContainerDimensionFormatter" />
	            <post class="PHPPdf\Formatter\StandardPositionFormatter" />
	            <post class="PHPPdf\Formatter\FloatFormatter" />
	        </formatters>
	    </node>
	    <node name="ol" class="PHPPdf\Node\BasicList">
	    	<stylesheet>
	    		<attribute name="type" value="decimal" />
	    		<attribute name="margin-left" value="12px" />
	    	</stylesheet>
	    	<invoke method="setEnumerationStrategyFactory" argId="list.enumeration_factory" />
	        <formatters>
	            <pre class="PHPPdf\Formatter\ConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\StandardDimensionFormatter" />
	            <pre class="PHPPdf\Formatter\FirstPointPositionFormatter" />
	            <pre class="PHPPdf\Formatter\ListFormatter" />

	            <post class="PHPPdf\Formatter\ContainerDimensionFormatter" />
	            <post class="PHPPdf\Formatter\StandardPositionFormatter" />
	            <post class="PHPPdf\Formatter\FloatFormatter" />
	        </formatters>
	    </node>
	    <node name="li" class="PHPPdf\Node\BasicListItem">
	    	<stylesheet>
	    		<attribute name="margin-left" value="10px" />
	    	</stylesheet>
	        <formatters>
	            <pre class="PHPPdf\Formatter\ConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\StandardDimensionFormatter" />
	            <pre class="PHPPdf\Formatter\FirstPointPositionFormatter" />

	            <post class="PHPPdf\Formatter\ContainerDimensionFormatter" />
	            <post class="PHPPdf\Formatter\StandardPositionFormatter" />
	            <post class="PHPPdf\Formatter\FloatFormatter" />
	        </formatters>
	    </node>
	    <node name="p" class="PHPPdf\Node\Container">
	        <stylesheet>
	            <attribute name="padding" value="12px 0" />
	        </stylesheet>
	        <formatters>
	            <pre class="PHPPdf\Formatter\ConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\StandardDimensionFormatter" />
	            <pre class="PHPPdf\Formatter\FirstPointPositionFormatter" />

	            <post class="PHPPdf\Formatter\ContainerDimensionFormatter" />
	            <post class="PHPPdf\Formatter\StandardPositionFormatter" />
	            <post class="PHPPdf\Formatter\FloatFormatter" />
	            <post class="PHPPdf\Formatter\VerticalAlignFormatter" />
	        </formatters>
	    </node>
	    
	    <node name="paragraph" class="PHPPdf\Node\Paragraph">
	        <formatters>
	            <pre class="PHPPdf\Formatter\ConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\StandardDimensionFormatter" />
	            <pre class="PHPPdf\Formatter\FirstPointPositionFormatter" />

	            <post class="PHPPdf\Formatter\ParagraphFormatter" />
	            <post class="PHPPdf\Formatter\ContainerDimensionFormatter" />
	            <post class="PHPPdf\Formatter\StandardPositionFormatter" />

	        </formatters>
	    </node>
	    
	    <node name="text" class="PHPPdf\Node\Text">
	    	<alias>span</alias>
	    	<alias>code</alias>
	        <formatters>
	            <pre class="PHPPdf\Formatter\ConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\TextDimensionFormatter" />
	        </formatters>
	    </node>
	    <node name="h1" class="PHPPdf\Node\Container">
	        <stylesheet>
	            <attribute name="font-size" value="18px" />
	            <attribute name="font-style" value="bold" />
	            <attribute name="margin" value="12px 0" />
	        </stylesheet>
	        <formatters>
	            <pre class="PHPPdf\Formatter\ConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\StandardDimensionFormatter" />
	            <pre class="PHPPdf\Formatter\FirstPointPositionFormatter" />

	            <post class="PHPPdf\Formatter\ContainerDimensionFormatter" />
	            <post class="PHPPdf\Formatter\StandardPositionFormatter" />
	            <post class="PHPPdf\Formatter\FloatFormatter" />
	            <post class="PHPPdf\Formatter\VerticalAlignFormatter" />
	        </formatters>
	    </node>
	    <node name="h2" class="PHPPdf\Node\Container">
	        <stylesheet>
	            <attribute name="font-size" value="16px" />
	            <attribute name="font-style" value="bold" />
	            <attribute name="margin" value="12px 0" />
	        </stylesheet>
	        <formatters>
	            <pre class="PHPPdf\Formatter\ConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\StandardDimensionFormatter" />
	            <pre class="PHPPdf\Formatter\FirstPointPositionFormatter" />

	            <post class="PHPPdf\Formatter\ContainerDimensionFormatter" />
	            <post class="PHPPdf\Formatter\StandardPositionFormatter" />
	            <post class="PHPPdf\Formatter\FloatFormatter" />
	            <post class="PHPPdf\Formatter\VerticalAlignFormatter" />
	        </formatters>
	    </node>
	    <node name="h3" class="PHPPdf\Node\Container">
	        <stylesheet>
	            <attribute name="font-size" value="14px" />
	            <attribute name="font-style" value="bold" />
	            <attribute name="margin" value="12px 0" />
	        </stylesheet>
	        <formatters>
	            <pre class="PHPPdf\Formatter\ConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\StandardDimensionFormatter" />
	            <pre class="PHPPdf\Formatter\FirstPointPositionFormatter" />

	            <post class="PHPPdf\Formatter\ContainerDimensionFormatter" />
	            <post class="PHPPdf\Formatter\StandardPositionFormatter" />
	            <post class="PHPPdf\Formatter\FloatFormatter" />
	            <post class="PHPPdf\Formatter\VerticalAlignFormatter" />
	        </formatters>
	    </node>
	    <node name="h4" class="PHPPdf\Node\Container">
	    	<alias>h5</alias>
	    	<alias>h6</alias>
	        <stylesheet>
	            <attribute name="font-size" value="12px" />
	        </stylesheet>
	        <formatters>
	            <pre class="PHPPdf\Formatter\ConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\StandardDimensionFormatter" />
	            <pre class="PHPPdf\Formatter\FirstPointPositionFormatter" />

	            <post class="PHPPdf\Formatter\ContainerDimensionFormatter" />
	            <post class="PHPPdf\Formatter\StandardPositionFormatter" />
	            <post class="PHPPdf\Formatter\FloatFormatter" />
	            <post class="PHPPdf\Formatter\VerticalAlignFormatter" />
	        </formatters>
	    </node>
	    <node name="page-info" class="PHPPdf\Node\Runtime\PageInfo">
	        <formatters>
	            <pre class="PHPPdf\Formatter\ConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\TextDimensionFormatter" />
	        </formatters>
	    </node>
	    <node name="page-number" class="PHPPdf\Node\Runtime\CurrentPageNumber">
	        <formatters>
	            <pre class="PHPPdf\Formatter\ConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\TextDimensionFormatter" />
	        </formatters>
	    </node>
	    <node name="table" class="PHPPdf\Node\Table">
	        <formatters>
	            <pre class="PHPPdf\Formatter\ConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\StandardDimensionFormatter" />
	            <pre class="PHPPdf\Formatter\TableColumnFormatter" />
	            <pre class="PHPPdf\Formatter\FirstPointPositionFormatter" />

	            <post class="PHPPdf\Formatter\ContainerDimensionFormatter" />
	            <post class="PHPPdf\Formatter\TableFormatter" />
	            <post class="PHPPdf\Formatter\StandardPositionFormatter" />
	        </formatters>
	    </node>
	    <node name="tr" class="PHPPdf\Node\Table\Row">
	        <stylesheet>
	            <attribute name="height" value="5px" />
	        </stylesheet>
	        <formatters>
	            <pre class="PHPPdf\Formatter\ConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\StandardDimensionFormatter" />
	            <pre class="PHPPdf\Formatter\FirstPointPositionFormatter" />

	            <post class="PHPPdf\Formatter\ContainerDimensionFormatter" />
	            <post class="PHPPdf\Formatter\StandardPositionFormatter" />
	            <post class="PHPPdf\Formatter\RowDimensionFormatter" />
	        </formatters>
	    </node>
	    <node name="td" class="PHPPdf\Node\Table\Cell">
	        <stylesheet>
	            <enhancement name="border" color="black" size="1px" />
	        </stylesheet>
	        <formatters>
	            <pre class="PHPPdf\Formatter\ConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\StandardDimensionFormatter" />
	            <pre class="PHPPdf\Formatter\CellFirstPointPositionFormatter" />

	            <post class="PHPPdf\Formatter\ContainerDimensionFormatter" />
	            <post class="PHPPdf\Formatter\StandardPositionFormatter" />
	            <post class="PHPPdf\Formatter\FloatFormatter" />
	        </formatters>
	    </node>
	    <node name="img" class="PHPPdf\Node\Image">
	        <stylesheet>
	            <attribute name="margin" value="1" />
	        </stylesheet>
	        <formatters>
	            <pre class="PHPPdf\Formatter\ImageConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\FirstPointPositionFormatter" />
	            <post class="PHPPdf\Formatter\StandardPositionFormatter" />
	        </formatters>
	    </node>
	    <node name="hr" class="PHPPdf\Node\Container">
	        <stylesheet>
	            <attribute name="height" value="1px" />
	            <attribute name="margin" value="12px 0" />
	            <enhancement name="background" color="black" />
	        </stylesheet>
	        <formatters>
	            <pre class="PHPPdf\Formatter\ConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\StandardDimensionFormatter" />
	            <pre class="PHPPdf\Formatter\FirstPointPositionFormatter" />

	            <post class="PHPPdf\Formatter\ContainerDimensionFormatter" />
	            <post class="PHPPdf\Formatter\StandardPositionFormatter" />
	            <post class="PHPPdf\Formatter\FloatFormatter" />
	        </formatters>
	    </node>
	    <node name="a" class="PHPPdf\Node\Text">
	    	<stylesheet>
	    		<attribute name="text-decoration" value="underline" />
	    		<attribute name="color" value="blue" />
	    	</stylesheet>
	        <formatters>
	            <pre class="PHPPdf\Formatter\ConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\TextDimensionFormatter" />
	        </formatters>
	    </node>
	    <node name="b" class="PHPPdf\Node\Text">
	    	<alias>strong</alias>
	        <stylesheet>
	            <attribute name="font-style" value="bold" />
	        </stylesheet>
	        <formatters>
	            <pre class="PHPPdf\Formatter\ConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\TextDimensionFormatter" />
	        </formatters>
	    </node>
	    <node name="i" class="PHPPdf\Node\Text">
	    	<alias>em</alias>
	        <stylesheet>
	            <attribute name="font-style" value="italic" />
	        </stylesheet>
	        <formatters>
	            <pre class="PHPPdf\Formatter\ConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\TextDimensionFormatter" />
	        </formatters>
	    </node>
	    <node name="page-break" class="PHPPdf\Node\Container">
	    	<alias>break</alias>
	    	<alias>column-break</alias>
	        <stylesheet>
	            <attribute name="break" value="1" />
	            <attribute name="height" value="0.01" />
	        </stylesheet>
	        <formatters>
	            <pre class="PHPPdf\Formatter\ConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\StandardDimensionFormatter" />
	            <pre class="PHPPdf\Formatter\FirstPointPositionFormatter" />

	            <post class="PHPPdf\Formatter\ContainerDimensionFormatter" />
	            <post class="PHPPdf\Formatter\StandardPositionFormatter" />
	            <post class="PHPPdf\Formatter\FloatFormatter" />
	        </formatters>
	    </node>
	    <node name="column-layout" class="PHPPdf\Node\ColumnableContainer">
	        <formatters>
	            <pre class="PHPPdf\Formatter\ConvertAttributesFormatter" />
	            <pre class="PHPPdf\Formatter\StandardDimensionFormatter" />
	            <pre class="PHPPdf\Formatter\FirstPointPositionFormatter" />

	            <post class="PHPPdf\Formatter\ContainerDimensionFormatter" />
	            <post class="PHPPdf\Formatter\StandardPositionFormatter" />
	            <post class="PHPPdf\Formatter\FloatFormatter" />
	        </formatters>
	    </node>
	</nodes>
</factory>
