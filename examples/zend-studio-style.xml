<?xml version="1.0" encoding="UTF-8"?>
<stylesheet>
	<div class="cover" background.image="dir:/zend-studio/cover.jpg" background.image-width="100%" height="400" color="white" vertical-align="top">
		<p margin="50 50" font-size="18">
			<h1 font-size="32" font-style="normal" padding="0" margin="0"></h1>
		</p>
	</div>
	
	<dynamic-page margin="40 70">
		<h1 font-size="30" font-style="normal" color="#00709e"></h1>
		<h2 color="#00709e"></h2>
		<p font-size="11" line-height="18"></p>
		<li font-size="11" line-height="18"></li>
		<div font-size="11" line-height="18"></div>
		<ul position="inside" type="circle" margin-left="20">
			<li margin-top="2" margin-bottom="2" />
			<ul margin-left="0" />
		</ul>
		
		<div class="note" margin-top="5">
			<div class="header" background.color="#838383" color="white" font-style="bold" padding="2">
			</div>
			<div class="content" background.color="#e5e5e5" padding="2">
			</div>
		</div>
		
		<div class="margin" height="20"></div>
		
		<table margin-top="5">
			<tr class="header" font-style="bold" />
			<td padding="5" border.size="0.5" />
			<ul margin-left="0" />
		</table>
		
		<div class="image" width="75%" margin="0 auto" text-align="center">
			<img width="100%" />
			<p font-size="9" />
		</div>
		
		<column-layout>
			<p line-height="16" padding="0" />
		</column-layout>
	</dynamic-page>
</stylesheet>